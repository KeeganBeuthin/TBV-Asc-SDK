{
  "name": "tbv-asc-sdk",
  "version": "1.0.5",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
],
  "scripts": {
    "asbuild:debug": "asc src/index.ts --target debug",
    "asbuild:release": "asc src/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "test": "node tests",
    "start": "npx serve .",
    "prebuild": "mkdir -p dist && ls -la dist",
    "build": "asc src/index.ts -b dist/index.wasm -t dist/index.wat --importMemory --sourceMap --optimize --verbose",
    "postbuild": "node scripts/generate-js-bindings.js"
  },
  "keywords": ["assemblyscript", "sdk", "TBV","tbv"],
  "author": "Keegan Beuthin",
  "license": "ISC",
  "devDependencies": {
    "@assemblyscript/loader": "^0.27.29",
    "assemblyscript": "^0.27.29"
  }
}