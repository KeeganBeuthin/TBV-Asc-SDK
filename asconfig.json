{
  "targets": {
    "debug": {
      "outFile": "dist/index.wasm",
      "textFile": "dist/index.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "dist/index.wasm",
      "textFile": "dist/index.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 1,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "bindings": "esm",
    "importMemory": true
  }
}